{
  "name": "@grass-kit/tailwind",
  "version": "0.0.6",
  "description": "A react component library based on Tailwind CSS",
  "keywords": [
    "tailwind",
    "tailwind-css",
    "bem",
    "css",
    "component",
    "ui-library"
  ],
  "homepage": "https://github.com/grass-kit/grass-kit",
  "bugs": {
    "url": "https://github.com/grass-kit/grass-kit/issues"
  },
  "repository": "https://github.com/grass-kit/grass-kit/tree/master/packages/taillwind",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "prepublishOnly": "npm-multi-publish",
    "postpublish": "npm-multi-publish",
    "serve": "live-server ./dev",
    "build": "echo 'ok'",
    "dev:build": "tailwindcss -c ./dev/tailwind.config.js -o ./dev/dist/tailwind.css",
    "dev:watch": "run-s 'dev:build -w'",
    "dev": "run-p dev:watch serve"
  },
  "devDependencies": {
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "tailwindcss": ">=3.0.0"
  },
  "peerDependencies": {
    "tailwindcss": ">=3.0.0"
  },
  "publishConfig": [
    {
      "registry": "https://registry.npmjs.org"
    },
    {
      "registry": "https://npm.pkg.github.com"
    }
  ]
}
